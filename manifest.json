{
  "name": "GTest",
  "version": "1.0",
  "description": "Google Chrome Extension that shares your sites to a remote site",
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "icon.png",
	"default_title": "Share!"
  },
  "permissions": ["tabs", "http://api.flickr.com/", "http://tech.morgadinho.org/gtest.php"]
}